{
  "name": "cdk-typescript-lambda-chromium",
  "version": "0.1.0",
  "bin": {
    "cdk-typescript-lambda-chromium": "bin/cdk-typescript-lambda-chromium.js"
  },
  "scripts": {
    "build": "tsc && npm run build-src",
    "build-src": "cd src && npm install && npm run build",
    "package": "cd src && npm run package",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "format": "prettier --write '{**/*.ts, **/**/*.ts}'",
    "lint": "eslint ./**/*.ts --fix"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "20.11.14",
    "aws-cdk": "2.126.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.126.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.4.5",
    "source-map-support": "^0.5.21"
  }
}
